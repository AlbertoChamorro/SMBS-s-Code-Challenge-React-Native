    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "node"
